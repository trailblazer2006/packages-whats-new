{
  "name": "packages-whats-new",
  "version": "0.0.1",
  "description": "What's new in your NPM packages",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node --project ./tsconfig.json ./src/index.ts 1w",
    "test": "test",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,ts}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts}"
  },
  "author": "AlexanderS",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cli-progress": "^3.8.0",
    "@types/node": "^14.14.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@supercharge/promise-pool": "^1.6.0",
    "app-root-path": "^3.0.0",
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "package-json": "^6.5.0",
    "read-package-json": "^3.0.0"
  }
}
